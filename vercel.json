{
    "version": 2,
    "builds": [
        {
            "src": "student_portal/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12.4"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "student_portal/wsgi.py"
        }
    ]
}